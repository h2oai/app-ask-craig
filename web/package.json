{
  "name": "h2o-app-ask-craig",
  "version": "0.0.1",
  "description": "Web app server for H2O sample",
  "main": "index.js",
  "scripts": {
    "start": "coffee server.coffee",
    "fluid": "fluid --compile app.coffee --include-js lib/thrift/lib/js/src/thrift.js --include-js gen-js/Web.js --include-js gen-js/web_types.js",
    "watch": "fluid --compile app.coffee --include-js lib/thrift/lib/js/src/thrift.js --include-js gen-js/Web.js --include-js gen-js/web_types.js --watch",
    "thrift": "thrift -gen js:node ../workflow/src/main/thrift/askcraig.thrift && thrift -gen js web.thrift && thrift -gen js:node web.thrift",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h2oai/app-ask-craig"
  },
  "keywords": [
    "h2o",
    "machine-learning",
    "predictive",
    "statistics"
  ],
  "author": "Prithvi Prabhu <prithvi@h2o.ai> (http://h2o.ai/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h2oai/app-ask-craig/issues"
  },
  "homepage": "https://github.com/h2oai/app-ask-craig",
  "dependencies": {
    "bower": "^1.4.1",
    "coffee-script": "^1.9.3",
    "fluid-lang": "0.0.3",
    "minimist": "^1.1.3",
    "mongodb": "^2.0.42",
    "thrift": "^0.9.2",
    "underscore": "^1.8.3"
  }
}
